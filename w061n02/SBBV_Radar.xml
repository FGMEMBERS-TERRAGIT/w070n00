<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
<path>SBBV_Radar.ac</path>
<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>16000</max-m>
</animation>
<animation>
<type>alpha-test</type>
<alpha-factor>0.01</alpha-factor>
</animation>
<animation><type>rotate</type>
<object-name>Radar</object-name>
<property>/sim/time/elapsed-sec</property>
<factor>60.0</factor>
<center>
<x-m>1.78</x-m>
<y-m>1.92</y-m>
<z-m>17.50</z-m>
</center>
<axis>
<x>0</x>
<y>0</y>
<z>-1</z>
</axis>
</animation>
<animation>
<type>material</type> 
<object-name>Radar</object-name>
<condition>		
<greater-than>		
<property>/sim/time/sun-angle-rad</property>
<value>1.57</value>
</greater-than>
</condition>
<emission>	
<red>0.3</red>	
<green>0.3</green>	
<blue>0.3</blue>
</emission>
</animation>
</PropertyList>